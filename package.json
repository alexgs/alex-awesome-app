{
  "name": "phils-awesome-app",
  "version": "1.3.0",
  "description": "Phil's \"Awesome App\" starter kit for Webpack and React",
  "main": "./src/index.js",
  "author": "Phil Gates-Shannon <phil.gs@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/philgs/phils-awesome-app.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --colors",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ]
      },
      "production": {
        "plugins": [
          [
            "transform-react-remove-prop-types",
            {
              "removeImport": true
            }
          ]
        ]
      }
    },
    "presets": [
      [
        "env",
        {
          "modules": false,
          "useBuiltIns": true
        }
      ],
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "browserslist": [
    "last 4 chrome versions",
    "last 4 chromeandroid versions",
    "last 4 edge versions",
    "last 4 firefox versions",
    "last 4 ios versions",
    "last 4 safari versions"
  ],
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-pro-light": "^5.0.13",
    "@fortawesome/fontawesome-pro-regular": "^5.0.13",
    "@fortawesome/fontawesome-pro-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.17",
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.24.1",
    "bootstrap": "^4.2.1",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.7.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.5"
  }
}
